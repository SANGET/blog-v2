<header class="header">
  <div class="container">
  <div class="row">
    <div class="col-sm-4">
      <span class="logo">{ Ukelli }</span>
    </div>
    <div class="col-sm-8">
      <nav class="main-nav">
        <ul>
          <% for (let i in get_config('menu')) { let menu = get_config('menu')[i]; %>
          <li class="nav-item<% if (typeof(page.path) !== 'undefined' && is_same_link(menu, page.path)) { %> is-active<% } %>"><a href="<%- url_for(menu) %>"><%= i %></a></li>
          <% } %>
        </ul>
      </nav>
    </div>
  </div>
  </div>
</header>